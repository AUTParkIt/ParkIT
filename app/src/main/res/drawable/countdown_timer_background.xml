<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp"
        android:top="15dp">
        <shape android:shape="oval" >
            <gradient
                android:angle="90"
                android:endColor="#f04500"
                android:startColor="#ffc752"
                android:type="linear"
                android:useLevel="false" />
            <size android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>

    <item
        android:bottom="35dp"
        android:left="35dp"
        android:right="35dp"
        android:top="35dp">
        <shape android:shape="oval" >
            <solid android:color="@color/colorWhite" />
        </shape>
    </item>
</layer-list>